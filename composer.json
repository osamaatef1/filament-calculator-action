{
    "name": "osama-dev/filament-calculator-action",
    "description": "Dynamic real-time calculator action for Filament v3 & v4 â€” instant client-side arithmetic, zero Livewire round-trips",
    "type": "library",
    "license": "MIT",
    "keywords": ["filament", "laravel", "alpine", "calculator", "action", "filament-plugin"],
    "authors": [
        {
            "name": "Osama",
            "email": "osama@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/filament": "^3.0|^4.0",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0|^10.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0"
    },
    "autoload": {
        "files": [
            "src/bootstrap.php"
        ],
        "psr-4": {
            "OsamaDev\\FilamentCalculatorAction\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OsamaDev\\FilamentCalculatorAction\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "OsamaDev\\FilamentCalculatorAction\\CalculatorActionServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "platform": {
            "ext-zip": "1.19.0"
        }
    }
}
